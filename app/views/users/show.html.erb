<p id="notice"><%= notice %></p>

<p>
<strong>Username: </strong>
<%= @user.name %> <br> <br> 

  <strong>Events organized:</strong>
  <%= @user.events.each do |event| %>
    <br> <%= link_to event.name, @event %>
  <% end %>
</p>

<p> <strong>Previous Events attending:</strong>
<%= @user.attended_events.previous_events.each do |event| %>
  <br> <%= link_to event.name, @event %>
  <% end %>
</p>

<p> <strong>Future Events attending:</strong>
  <%= @user.attended_events.future_events.each do |event| %>
    <br> <%= link_to event.name, @event %>
  <% end %>


<%= link_to 'Back', users_path %>
 